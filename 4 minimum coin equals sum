#include <iostream>
#include <bits/stdc++.h>
using namespace std;

void fun(vector <int> &a,int n,int sum,int ind,int count,int *max)

{
    if(ind==n || sum<0)
    return;
    if(sum==0)
    {
         if(count<(*max))
         {
              *max=count;
              return;
         }
    }
    if(*max>count+1)
    fun(a,n,sum-a[ind],ind+1,count+1,max);
    if(*max>count+1)
    fun(a,n,sum-a[ind],ind,count+1,max);
    if(*max>count)
    fun(a,n,sum,ind+1,count,max);
   return ;
}

int main() {
	int n,sum,i,max,k;
	max=INT_MAX;
	cin>>n>>sum;
  vector<int> a;
	for(i=0;i<n;i++)
	{
	cin>>k;
	a.push_back(k);
	}
	sort(a.begin(), a.end(), greater<int>());
	fun(a,n,sum,0,0,&max);
	cout<<max;
	return 0;
}


int fun(int a[],int n,int sum)
{
     if(sum==0)
     return 0;
     int ans=INT_MAX;
     int i,res;
     for(i=n-1;i>=0;i--)
     {
          if(a[i]<=sum)
          {
               res=fun(a,n,sum-a[i]);
               if(res!=INT_MAX && res+1<ans)
               ans=res+1;
          }
     }
     return ans;
}

int main() {
	int i,n,k;
	cin>>n>>k;
	int a[n];
	for(i=0;i<n;i++)
	cin>>a[i];
	sort(a,a+n);
	cout<<fun(a,n,k);
	return 0;
}
