#include <iostream>
#include <bits/stdc++.h>
using namespace std;

void fun(vector <int> &a,int n,int sum,int ind,int count,int *max)

{
    if(ind==n || sum<0)
    return;
    if(sum==0)
    {
         if(count<(*max))
         {
              *max=count;
              return;
         }
    }
    fun(a,n,sum,ind+1,count,max);
    fun(a,n,sum-a[ind],ind+1,count+1,max);
    fun(a,n,sum-a[ind],ind,count+1,max);
   return ;
}

int main() {
	int n,sum,i,max,k;
	max=INT_MAX;
	cin>>n>>sum;
  vector<int> a;
	for(i=0;i<n;i++)
	{
	cin>>k;
	a.push_back(k);
	}
	sort(a.begin(), a.end(), greater<int>());
	fun(a,n,sum,0,0,&max);
	cout<<max;
	return 0;
}
