#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	int i,n;
	cin>>n;
	int a[n];
	for(i=0;i<n;i++)
	cin>>a[i];
	int c,me;
	c=1;
	me=a[0];
	for(i=1;i<n;i++)
	{
	     if(c==0)
	     {
	          me=a[i];
	          c=1;
	     }
	     else
	     {
	          if(a[i]==me)
	          {
	               c++;
	          }
	          else
	          c--;
	     }
	}
	c=0;
	for(i=0;i<n;i++)
	{
	   if(a[i]==me)
	   c++;
	}
//cout<<me<<" "<<c<<endl;
	if(c>(n/2))
	cout<<me;
	else
	cout<<"-1";
	return 0;
}
