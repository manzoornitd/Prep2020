#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	int i,j,n,m,k;
	cin>>n>>m>>k;
	int a[n][m];
	for(i=0;i<n;i++)
	{
	     for(j=0;j<m;j++)
	     cin>>a[i][j];
	}
	i=0;
	j=m-1;
	while(i<n && j>=0)
	{
	     cout<<i<<" "<<j<<endl;
	     if(a[i][j]==k)
	     {
	          cout<<"found at "<<i<<" "<<j;
	          break;
	     }
	     if(a[i][j]>k)
	     j--;
	     else if(a[i][j]<k)
	     i++;
	}
	
	return 0;
}
