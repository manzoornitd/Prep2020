#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	int i,j,n;
	cin>>n;
int arr[n][n];
	for(i=0;i<n;i++)
	{
	     for(j=0;j<n;j++)
	     {
	     cin>>arr[i][j];
	     }
	}
	for(i=0;i<n;i++)
	for(j=0;j<n;j++)
	cout<<arr[i][j]<<" ";
	cout<<endl;
	int sum=0;
		for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
	     {
	    int x,y;
	    x=(i+1)*(j+1);
	    y=(n-i)*(n-j);
	    sum+=arr[i][j]*(x*y);
	     }
	}
	cout<<sum;
	return 0;
}
