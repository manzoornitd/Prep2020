
int main() {
	int i,n,j;
	cin>>n;
	int a[n];
	for(i=0;i<n;i++)
	cin>>a[i];
	i=0;
	j=0;
	for(i=0;i<n;i++)
	{
	     if(i<n-2 && a[i]==a[i+1] && a[i]==a[i+2])
	     continue;
	     else
	     {
	          a[j]=a[i];
	          j++;
	          
	     }
	}
	for(i=0;i<j;i++)
	cout<<a[i]<<" ";
	return 0;
}
