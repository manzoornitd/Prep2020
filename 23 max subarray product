
int main() {
	int i,mx,mi,ans,temp,n,flag=0;
	cin>>n;
	int a[n];
	for(i=0;i<n;i++)
	cin>>a[i];
     mx=a[0];
     mi=a[0];
     ans=a[0];
     for(i=1;i<n;i++)
     {
          if(a[i]<0)
          {
               temp=mx;
               mx=mi;
               mi=temp;
          }
          mx=max(a[i],a[i]*mx);
          mi=min(a[i],a[i]*mi);
          ans=max(ans,mx);
     }
     if(ans>=0)
	cout<<ans;
	else
	cout<<"0";
	return 0;
}
