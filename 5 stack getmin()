#include <iostream>
#include <bits/stdc++.h>                   // using 2 stacks is easy just push the min and for new min push new min
using namespace std;                      // here one stack is used by 2*x-min and 2*min-top();

class getmin
{
     public:
     stack <int> st;
     int min;
     void push(int x)
     {
          if(st.size()==0)
          {
               st.push(x);
               min=x;
          }
          else if (x>min)
          {
               st.push(x);
          }
          else
          {
               st.push(2*x-min);
               min=x;
          }
          return ;
     }
     void pop()
     {
         if(st.size()==0)
         cout<<"empty";
        else if(min<st.top())
         {
             cout<<min<<endl;
             st.pop();
         }
         else
         {
              int x=min;
              min=2*min-st.top();
              cout<<x<<endl;
              st.pop();
         }
         return ;
         
     }
};

int main() {
	int i,n;
	cin>>n;
  getmin a;
	for(i=0;i<n;i++)
  {
       int x;
       cin>>x;
       a.push(x);
  }
  a.pop();
    a.pop();
      a.pop();
        a.pop();
          
	return 0;
}
