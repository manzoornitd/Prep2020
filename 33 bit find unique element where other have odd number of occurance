#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	int i,n;                          // every element appear 3 times except one ,find that element 
	cin>>n;
	int *arr= new int[n];
	for(i=0;i<n;i++)
	cin>>arr[i];
	
	int sum=0;
	int c,p,z=0;
	while(1)
	{
	     int k=0;
	     c=0;
	     for(i=0;i<n;i++)
	     {
	         p=arr[i]&1;
	         if(p==0)
	         c++;
	         arr[i]=arr[i]>>1;
	         if(arr[i]==0)
	         k++;
	         
	     }
	     if(c%3==0)
	     sum+=pow(2,z);
	     z++;
	     if(k==n)
	     break;
	}
	cout<<sum<<endl;
	return 0;
}
