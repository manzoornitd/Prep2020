int main() {
	int i,j,n,sum,k;
	cin>>n>>k;
	sum=0;
	int *a= new int[n];
	for(i=0;i<n;i++)
	{
	     cin>>a[i];
	}
	i=0; j=n-1;
	while(i<=j)
	{
	     if(a[i]*a[j]<k)
	     {
	          
	          sum+=(j-i)*2 +1;
	          i++;
	     }
	     else 
	     {
	          j--;
	     }
	}
	cout<<sum;
	return 0;
}
