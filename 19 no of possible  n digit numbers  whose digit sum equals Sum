#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int dp[100][200];
// no of possible n digit  number whose sum equals Sum; 
int fun2(int n,int sum)
{
     if(n==0)
     {
          if(sum==0)
          return 1;
          else
          return 0;
     }
     if(dp[n][sum]!=-1)
     return dp[n][sum];
     int i,ans=0;
     for(i=0;i<=9;i++)
     {
        if(sum-i>=0)
        {
             ans+=fun2(n-1,sum-i);
        }
        
     }
     dp[n][sum]=ans;
     return ans;
}

void fun(int n,int sum)
{
     
     
     memset(dp,-1,sizeof(dp));
     int i,ans=0;

     for(i=1;i<10;i++)
     {
          if(sum-i>=0)
          ans+=fun2(n-1,sum-i);
     }
     cout<<ans;
}


int main() {
	int n,sum;
	cin>>n>>sum;
	fun(n,sum);
	return 0;
}
